{
  "name": "ameh-web-components",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/ameh-web-components/ameh-web-components.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production stencil build --docs",
    "start": "cross-env NODE_ENV=development stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^2.0.1",
    "bootstrap-icons": "^1.2.2"
  },
  "license": "MIT",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "@stencil/postcss": "^2.0.0",
    "@types/node": "^14.14.20",
    "autoprefixer": "^10.1.0",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "tailwindcss": "^2.0.2"
  }
}
